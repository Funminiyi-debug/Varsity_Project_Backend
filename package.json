{
  "name": "versity-backend",
  "version": "1.0.0",
  "description": "versity api",
  "main": "dist/server.js",
  "scripts": {
    "test": "jest -c ./jest.unit.json",
    "test:coverage": "jest --coverage -c ./jest.unit.json",
    "dev": "concurrently \"npm run devStart\" \"nodemon -x tsoa spec\"",
    "start": "tsc && node dist/server.js",
    "prebuild": "npm run swagger",
    "devStart": "nodemon --watch src/**/*.ts  --exec ts-node ./src/server.ts",
    "swagger": "tsoa spec"
  },
  "dependencies": {
    "@decorators/express": "^2.4.0",
    "@types/cors": "^2.8.9",
    "@types/morgan": "^1.9.2",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "jest": "^26.6.3",
    "joi": "^17.3.0",
    "mongoose": "^5.10.16",
    "morgan": "^1.10.0",
    "multer": "^1.4.2",
    "nodemon": "^2.0.7",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "swagger-ui-express": "^4.1.6",
    "ts-node": "^9.0.0",
    "tslint": "^6.1.3",
    "tsoa": "^3.5.1"
  },
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.20",
    "@types/node": "^14.14.22",
    "@types/swagger-ui-express": "^4.1.2",
    "concurrently": "^5.3.0",
    "ts-jest": "^26.5.0",
    "typescript": "^4.1.2"
  },
  "author": "",
  "license": "ISC"
}
