    "axios": "^0.21.1",
    "bcrypt": "^5.0.0",
    "body-parser": "^1.19.0",
    "cookie-session": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "express-validator": "^6.9.2",
    "inversify": "^5.0.5",
    "inversify-express-utils": "^6.3.2",
    "jest": "^26.6.3",
    "joi": "^17.3.0",
    "jwthelper": "0.0.4",
